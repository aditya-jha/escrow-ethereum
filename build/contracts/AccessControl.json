  "source": "pragma solidity ^0.4.18;\n\n\nimport \"./Helpers.sol\";\n\n\ncontract AccessControl is Helpers {\n\n\n    address public owner;\n\n    function AccessControl() public {\n        owner = msg.sender;\n    }\n\n    modifier onlyOwner() {\n        require(msg.sender == owner);\n        _;\n    }\n}",
